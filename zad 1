using System;

namespace zad2
{
    class Program
    {
        static void Main(string[] args)
        {
            int a;
            Console.WriteLine("podaj liczbe:");
            a = Convert.ToInt32(Console.ReadLine());
            if (a % 2 == 0) Console.WriteLine("liczba jest podzielna przez 2");
            if (a % 3 == 0) Console.WriteLine("liczba jest podzielna przez 3");
            if (a % 4 == 0) Console.WriteLine("liczba jest podzielna przez 4");
            if (a % 5 == 0) Console.WriteLine("liczba jest podzielna przez 5");
            if (a % 6 == 0) Console.WriteLine("liczba jest podzielna przez 6");
            if (a % 7 == 0) Console.WriteLine("liczba jest podzielna przez 7");
            if (a % 8 == 0) Console.WriteLine("liczba jest podzielna przez 8");
            if (a % 9 == 0) Console.WriteLine("liczba jest podzielna przez 9");

            Console.ReadKey();
            



        }
    }
}
